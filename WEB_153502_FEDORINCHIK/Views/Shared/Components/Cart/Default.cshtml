@using WEB_153502_FEDORINCHIK.Domain.Models
@model Cart

@{
    double totalPrice = Model?.TotalPrice ?? 0;
    int count = Model?.Count ?? 0;
}

<a asp-controller="Cart" asp-action="Index" class="navbar-text ms-auto text-decoration-none">
    $@totalPrice  <i class="fa-solid fa-cart-shopping"></i> (@count)
</a>
